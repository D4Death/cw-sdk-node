{
  "name": "cw-stream-client",
  "version": "0.0.4",
  "description":
    "A library for managing connections to Cryptowatch Websocket Streaming API",
  "author": "Cryptowatch <infra@cryptowatch>",
  "homepage": "https://cryptowat.ch/docs/streaming-api",
  "license": "BSD-2-Clause-FreeBSD",
  "main": "dist/client.js",
  "files": "proto/*",
  "scripts": {
    "test": "jest",
    "build": "node_modules/typescript/bin/tsc -p tsconfig.build.json",
    "dist": "node_modules/typescript/bin/tsc -p tsconfig.dist.json"
  },
  "dependencies": {
    "@types/big.js": "^4.0.5",
    "@types/long": "^4.0.0",
    "@types/node": "^10.5.2",
    "@types/ws": "^5.1.2",
    "big.js": "^5.1.2",
    "long": "^4.0.0",
    "pjson": "^1.0.9",
    "protobufjs": "^6.8.6",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.1.6",
    "in-series": "0.0.4",
    "jest": "^23.4.1",
    "ts-jest": "^23.0.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.build.json"
      }
    }
  }
}
